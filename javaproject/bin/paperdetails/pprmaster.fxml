<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="600.0" minWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="paperdetails.pprmasterController">
   <children>
      <Label layoutX="218.0" layoutY="40.0" text="Paper Details" textFill="RED">
         <font>
            <Font name="Courier New Bold" size="21.0" />
         </font>
      </Label>
      <Label fx:id="lblname" layoutX="118.0" layoutY="201.0" prefHeight="25.0" prefWidth="91.0" text="Paper Name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="lblprice" layoutX="137.0" layoutY="301.0" prefHeight="25.0" prefWidth="53.0" text="Price">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Line endX="167.0" layoutX="300.0" layoutY="92.0" startX="-169.0" />
      <ComboBox fx:id="comboname" editable="true" layoutX="218.0" layoutY="196.0" onAction="#docombo" prefHeight="34.0" prefWidth="150.0" />
      <TextField fx:id="txtprice" layoutX="219.0" layoutY="296.0" prefHeight="34.0" prefWidth="149.0" />
      <Button fx:id="btnfetch" layoutX="433.0" layoutY="196.0" mnemonicParsing="false" onAction="#dofetch" prefHeight="34.0" prefWidth="64.0" style="-fx-background-color: blue; -fx-text-fill: white; -fx-background-radius: 8px;" text="FETCH">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <DropShadow radius="12.965" width="32.86" />
         </effect>
      </Button>
      <Button fx:id="btnremove" layoutX="433.0" layoutY="402.0" mnemonicParsing="false" onAction="#doremove" prefHeight="34.0" prefWidth="64.0" style="-fx-background-color: green; -fx-text-fill: white; -fx-background-radius: 8px;" text="REMOVE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Button fx:id="btnupdate" layoutX="318.0" layoutY="402.0" mnemonicParsing="false" onAction="#doupdate" prefHeight="34.0" prefWidth="64.0" style="-fx-background-color: orange; -fx-text-fill: black; -fx-background-radius: 8px;" text="UPDATE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Button fx:id="btnsave" layoutX="205.0" layoutY="402.0" mnemonicParsing="false" onAction="#dosave" prefHeight="34.0" prefWidth="64.0" style="-fx-background-color: green; -fx-text-fill: white; -fx-background-radius: 8px;" text="SAVE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Button fx:id="btnnew" layoutX="99.0" layoutY="402.0" mnemonicParsing="false" onAction="#donew" prefHeight="34.0" prefWidth="64.0" style="-fx-background-color: orange; -fx-text-fill: black; -fx-background-radius: 8px;" text="NEW">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <DropShadow />
         </effect>
      </Button>
      <Label fx:id="lblresult" layoutX="451.0" layoutY="305.0" />
   </children>
</AnchorPane>
